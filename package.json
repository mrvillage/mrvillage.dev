    "@tsparticles/slim": "^3.0.2",
    "cf-bindings-proxy": "^0.7.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "kysely": "^0.26.3",
    "kysely-d1": "^0.3.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18",
