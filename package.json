{
  "name": "mrvillage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"contentlayer dev\" \"next dev\"",
    "build": "contentlayer build && node contentlayer-patch.js && next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-06 --compatibility-flag=nodejs_compat",
    "proxy": "npx cf-bindings-proxy"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@mantine/hooks": "^7.3.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tabler/icons-react": "^2.44.0",
    "@tsparticles/engine": "^3.0.2",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/og": "^0.6.1",
    "cf-bindings-proxy": "^0.7.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.0.1",
    "kysely": "^0.26.3",
    "kysely-d1": "^0.3.0",
    "next": "14.0.4",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "shiki": "^0.14.7",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-next-on-pages": "^1.8.2",
    "postcss": "^8",
    "rehype": "^13.0.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.12.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vercel": "^32.7.2"
  },
  "overrides": {
    "next-contentlayer": {
      "next": "$next"
    }
  }
}
